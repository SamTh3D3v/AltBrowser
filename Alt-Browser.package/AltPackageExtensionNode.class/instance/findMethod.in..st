querying
findMethod: aSelector in: aClass
	"Filter out: force contents building if the class belongs to this package."

	| res |
	(self rpackage includesClass: aClass)
		ifTrue: [ 
			self contents
				do: [ :each | 
					(res := each findMethod: aSelector in: aClass) notNil
						ifTrue: [ ^ res ] ] ].
	^ nil